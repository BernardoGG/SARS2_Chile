library(tidyverse)
library(janitor)
library(lubridate)
library(stringr)
library(ggplot2)
library(scales)
library(patchwork)
library(NatParksPalettes)
lapply(1:length(taxalist),
function(i) write.csv(taxalist[[i]],
file = paste0(names(taxalist[i]), ".tsv"),
row.names = FALSE,
sep = "\t",
quote = FALSE)) # <-- Export TL tips to extract with fertree
warnings()
lapply(1:length(taxalist),
function(i) write.table(taxalist[[i]],
file = paste0(names(taxalist[i]), ".tsv"),
row.names = FALSE,
sep = "\t",
quote = FALSE)) # <-- Export TL tips to extract with fertree
